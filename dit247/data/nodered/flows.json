        "id": "894ebdc239c39b5f",
        "type": "function",
        "z": "1b034dd948f70bc1",
        "name": "File flow end/Duration",
        "func": "// Set the result to msg.extracted or any other property you want\nlet file = msg.payload.Key.replace('dit247c/', '').replace('-c', '');\n\n// Get file starttime from flow if there\nlet startTime = flow.get(file);\n\nif (startTime) {\n    let endTime = Date.now();\n    \n    // Calculate the time interval in milliseconds\n    let duration = endTime - startTime;\n\n    // Store the duration in the message payload to send it via POST request\n    msg.payload = {\n        file: file,\n        duration: duration\n    };\n\n    // Clear the stored start time for this key\n    flow.set(file, null);\n}\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
